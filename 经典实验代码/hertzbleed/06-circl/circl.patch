+// Code from Yingchen. 
+func Bytes2Bits(data []byte) []int {
+	dst := make([]int, 0)
+	first_one := int(0) 
+	for _, v := range data {
+		for i := 0; i < 8; i++ {
+			move := uint(7 - i)
+			to_Append := int((v>>move)&1)
+			if(to_Append == 1){
+			    first_one = 1			
+			}
+			if(first_one == 1){
+			    dst = append(dst, to_Append)
+			}
+		}
+	}
+	return dst
+}
